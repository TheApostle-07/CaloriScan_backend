{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_API_KEY": "@google_api_key",
    "PORT": "3000"
  }
}
